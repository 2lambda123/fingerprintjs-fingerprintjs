{
  "name": "fingerprintjs2",
  "version": "1.4.1",
  "description": "Modern & flexible browser fingerprinting library",
  "main": "dist/fingerprint2.min.js",
  "devDependencies": {
    "eslint": "^3.7.0",
    "uglify-js": "^2.7.3"
  },
  "scripts": {
    "test": "specs/phantomjs.runner.sh specs/spec_runner.html",
    "lint": "eslint fingerprint2.js",
    "minify": "rm -rf dist && mkdir dist && cd dist && uglifyjs ../fingerprint2.js -c global_defs={NODEBUG:true} -m -o fingerprint2.min.js",
    "build": "npm run lint && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Valve/fingerprintjs2.git"
  },
  "keywords": [
    "browser",
    "identification",
    "fingerprint",
    "fingerprinting",
    "privacy"
  ],
  "author": "Valentin Vasilyev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Valve/fingerprintjs2/issues"
  },
  "homepage": "https://github.com/Valve/fingerprintjs2"
}
